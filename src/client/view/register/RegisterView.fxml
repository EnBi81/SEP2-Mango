<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="client.view.register.RegisterText"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="248.0" layoutY="31.0" text="Register">
            <font>
                <Font size="30.0" />
            </font>
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </Label>
        <TextField fx:id="firstName" layoutX="177.0" layoutY="109.0" prefHeight="25.0" prefWidth="248.0">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </TextField>
        <RadioButton fx:id="radioCustomer" layoutX="177.0" layoutY="313.0" mnemonicParsing="false" text="Customer">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </RadioButton>
        <RadioButton fx:id="radioEmployee" layoutX="265.0" layoutY="313.0" mnemonicParsing="false" text="Employee">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </RadioButton>
        <TextField fx:id="lastName" layoutX="176.0" layoutY="148.0" prefHeight="25.0" prefWidth="248.0">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </TextField>
        <TextField fx:id="username" layoutX="176.0" layoutY="188.0" prefHeight="25.0" prefWidth="248.0">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </TextField>
        <Button layoutX="545.0" layoutY="361.0" mnemonicParsing="false" onAction="#onBack" text="Back">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <effect>
                <ColorAdjust />
            </effect>
        </Button>
        <Button layoutX="364.0" layoutY="309.0" mnemonicParsing="false" onAction="#onRegister" text="Register">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Label layoutX="14.0" layoutY="14.0" text="CanEat">
            <cursor>
                <Cursor fx:constant="DEFAULT" />
            </cursor>
            <effect>
                <ImageInput>
                    <source>
                        <Image url="logo/logo.jpg" />
                    </source>
                </ImageInput>
            </effect>
        </Label>
        <Label layoutX="107.0" layoutY="113.0" text="First Name:">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </Label>
        <Label layoutX="108.0" layoutY="152.0" text="Last Name:">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </Label>
        <Label layoutX="108.0" layoutY="192.0" text="Username:">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </Label>
        <Label layoutX="110.0" layoutY="234.0" text="Password:">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </Label>
        <Label layoutX="69.0" layoutY="271.0" text="Password (repeat):">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </Label>
        <PasswordField fx:id="password" layoutX="177.0" layoutY="230.0" prefHeight="25.0" prefWidth="248.0">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </PasswordField>
        <PasswordField fx:id="passwordRepeat" layoutX="176.0" layoutY="267.0" prefHeight="25.0" prefWidth="248.0">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </PasswordField>
        <Label fx:id="errorMessage" layoutX="266.0" layoutY="353.0" text="ErrorMessage" textFill="RED" />
    </children>
</AnchorPane>
