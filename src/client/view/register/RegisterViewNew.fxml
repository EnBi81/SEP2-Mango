                <Cursor fx:constant="TEXT" />
            </cursor>
        </TextField>
        <TextField fx:id="username" layoutX="176.0" layoutY="188.0" prefHeight="25.0" prefWidth="248.0">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </TextField>
        <Button layoutX="545.0" layoutY="361.0" mnemonicParsing="false" onAction="#onBack" text="Back">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Button layoutX="364.0" layoutY="309.0" mnemonicParsing="false" onAction="#onRegister" text="Register">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Label layoutX="14.0" layoutY="14.0" text="CanEat">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </Label>
        <Label layoutX="107.0" layoutY="113.0" text="First Name:">
            <cursor>
                <Cursor fx:constant="TEXT" />
