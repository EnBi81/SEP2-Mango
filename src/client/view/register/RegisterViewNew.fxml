        </Label>
        <TextField fx:id="firstName" layoutX="177.0" layoutY="109.0" prefHeight="25.0" prefWidth="248.0">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </TextField>
        <RadioButton fx:id="radioCustomer" layoutX="177.0" layoutY="313.0" mnemonicParsing="false" text="Customer">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </RadioButton>
        <RadioButton fx:id="radioEmployee" layoutX="265.0" layoutY="313.0" mnemonicParsing="false" text="Employee">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </RadioButton>
        <TextField fx:id="lastName" layoutX="176.0" layoutY="148.0" prefHeight="25.0" prefWidth="248.0">
