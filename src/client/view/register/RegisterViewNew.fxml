        </Label>
        <Label layoutX="108.0" layoutY="192.0" text="Username:">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </Label>
        <Label layoutX="110.0" layoutY="234.0" text="Password:">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </Label>
        <Label layoutX="69.0" layoutY="271.0" text="Password (repeat):">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </Label>
        <PasswordField fx:id="password" layoutX="177.0" layoutY="230.0" prefHeight="25.0" prefWidth="248.0">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </PasswordField>
        <PasswordField fx:id="passwordRepeat" layoutX="176.0" layoutY="267.0" prefHeight="25.0" prefWidth="248.0">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </PasswordField>
        <Label fx:id="errorMessage" layoutX="266.0" layoutY="353.0" text="ErrorMessage" textFill="RED" />
